@{
    ViewData["Title"] = "User";
    ViewData["UserNick"] = "Michal";
}

@model List<User>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Nick</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var user in Model){
        <tr>
            <td>@user.Id</td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Nick</td>
            <td>@user.Email</td>
        </tr>
        }
    </tbody>
</table>
